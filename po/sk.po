#: ../ui/regexxer.desktop.in.h:1
msgid "regexxer Search Tool"
msgstr "regexxer - vyhľadávací nástroj"

#: ../ui/regexxer.desktop.in.h:2 ../src/mainwindow.cc:1028
msgid "Search and replace using regular expressions"
msgstr "Vyhľadávať a nahrádzať pomocou regulárnych výrazov"

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:1
msgid "File Patterns"
msgstr "Vzory súborov"

# description File Patterns
#: ../ui/org.regexxer.gschema.xml.in.h:2
msgid "List of pre-defined patterns available in the 'Pattern' entry."
msgstr "Zoznam preddefinovaných vzorov dostupných v poli „Vzor“."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:3
msgid "Regex Patterns"
msgstr "Vzory regulárnych výrazov"

# description Regex Patterns
#: ../ui/org.regexxer.gschema.xml.in.h:4
msgid "List of last patterns used in the 'Regex' entry."
msgstr "Zoznam naposledy použitých vzorov v poli „Regulárny výraz“."

# description Regex Patterns
#: ../ui/org.regexxer.gschema.xml.in.h:5
msgid "List of last patterns used in the 'Substitution' entry."
msgstr "Zoznam naposledy použitých vzorov v poli „Nahradiť“."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:6
msgid "Text view font"
msgstr "Písmo zobrazeného textu"

# description Text view font
#: ../ui/org.regexxer.gschema.xml.in.h:7
msgid "The font used in the file editor."
msgstr "Písmo používané v editore súborov."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:8
msgid "Match color"
msgstr "Farba výskytu"

# description Match color
#: ../ui/org.regexxer.gschema.xml.in.h:9
msgid ""
"The background color used to highlight matches of the search expression."
msgstr "Farba pozadia použitá na zvýraznenie výskytov hľadaného výrazu."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:10
msgid "Current-match color"
msgstr "Farba aktuálne označeného výskytu"

# descrition Current-match color
#: ../ui/org.regexxer.gschema.xml.in.h:11
msgid ""
"The background color used to highlight the currently selected match of the "
"search expression."
msgstr ""
"Farba pozadia použitá na zvýraznenie práve označeného výskytu hľadaného "
"výrazu."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:12
msgid "Fallback encoding"
msgstr "Záložné kódovanie"

# description Fallback encoding
#: ../ui/org.regexxer.gschema.xml.in.h:13
msgid ""
"Name of the character encoding to use if a file is not readable in either "
"UTF-8 or the codeset specified by the current locale. Try \"iconv --list\" "
"for a complete list of possible values."
msgstr ""
"Názov kódovania znakov, ktoré sa má použiť, ak súbor nie je čitateľný v "
"kódovaní UTF-8, ani v kódovaní určenom aktuálnymi miestnymi nastaveniami. "
"Úplný zoznam možných hodnôt získate, zadaním príkazu „iconv --list“."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:14
msgid "window width"
msgstr "šírka okna"

# description window width
#: ../ui/org.regexxer.gschema.xml.in.h:15
msgid "The width of the window."
msgstr "Šírka okna."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:16
msgid "window height"
msgstr "výška okna"

# description window height
#: ../ui/org.regexxer.gschema.xml.in.h:17
msgid "The height of the window."
msgstr "Výška okna."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:18
msgid "window x position"
msgstr "x-ová súradnica okna"

# description window x position
#: ../ui/org.regexxer.gschema.xml.in.h:19
msgid "The X coordinate of the window."
msgstr "X-ová súradnica okna."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:20
msgid "window y position"
msgstr "y-ová súradnica okna"

# description window y position
#: ../ui/org.regexxer.gschema.xml.in.h:21
msgid "The Y coordinate of the window."
msgstr "Y-ová súradnica okna."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:22
msgid "window maximization"
msgstr "maximalizácia okna"

# description window maximization
#: ../ui/org.regexxer.gschema.xml.in.h:23
msgid "Whether the window is maximized."
msgstr "Či je okno maximalizované."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:24
msgid "Show line numbers"
msgstr "Zobraziť čísla riadkov"

# description Show line numbers
#: ../ui/org.regexxer.gschema.xml.in.h:25
msgid "Whether to show the line numbers of the text view."
msgstr "Či sa majú zobraziť čísla riadkov pri zobrazenom texte."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:26
msgid "Highlight the current line"
msgstr "Zvýrazniť aktuálny riadok"

# description Highlight the current line
#: ../ui/org.regexxer.gschema.xml.in.h:27
msgid "Whether to highlight the current line of the text view."
msgstr "Či sa má zvýrazniť aktuálny riadok v zobrazenom texte."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:28
msgid "Auto indentation"
msgstr "Automatické odsadzovanie"

# description Auto indentation
#: ../ui/org.regexxer.gschema.xml.in.h:29
msgid "Whether to enable auto indentation in the text view."
msgstr "Či sa má povoliť automatické odsadenie v zobrazenom texte."

# summary
#: ../ui/org.regexxer.gschema.xml.in.h:30
msgid "Draw spaces"
msgstr "Písanie medzier"

# description Draw spaces
#: ../ui/org.regexxer.gschema.xml.in.h:31
msgid "Which type of space to draw in the text view."
msgstr "Ktorý typ medzier sa má písať v zobrazenom texte."

